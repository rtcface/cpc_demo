---
import type { NuestroCompromisoEntry } from "@/types";
import EntryLayout from "@components/nuestroCompromiso/EntryLayout.astro";
import { getCollection } from "astro:content";

const entries = await getCollection("nuestroCompromiso");
const entry = entries.find(entry => entry.id === "mision") as NuestroCompromisoEntry;
---

